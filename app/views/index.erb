<h1>Music Wall V1</h1>
<a href="/user">Sign up</a>
<a href="/user/login">Login</a>
<p> </p>
<% Song.all.each do |song| %>
  <div class="song"> 
    <h3>Song: <%= song.songtitle %></h3>
    <h4> Band: <%= song.author %></h4>
    <p> Link: <%= song.url %></p>
  </div>
<% end %>



<a href="/upload">Submit a new song to the wall.</a>
